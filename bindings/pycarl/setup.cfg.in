[build_ext]
# uncomment the following to enable CLN
@PYCARL_DEFINE_CLN@
# uncomment the following to link with these libraries
# not required, as libcarl does so already
libraries=@PYCARL_LIBRARIES@
# uncomment the following to use carl lib from parent directory
include_dirs=@PYCARL_INCLUDE_DIRS@
library_dirs=@PYCARL_LIBRARY_DIRS@
rpath=@PYCARL_RPATH@
